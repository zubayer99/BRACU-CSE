    "#Task1\n",
    "def shiftLeft(source, k):\n",
    "    for i in range(0,k):\n",
    "        temp=source[0]\n",
    "        for j in range(0,len(source)-1):\n",
    "            source[j]=source[j+1]\n",
    "        source[len(source)-1]=temp\n",
    "\n",
    "source=[10,20,30,40,50,60]\n",
    "shiftLeft(source,3)"
   ]
